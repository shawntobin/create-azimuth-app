{
  "name": "id.urbit.org",
  "private": true,
  "version": "0.0.0",
  "engines": {
    "npm": "only-use-yarn",
    "node": "^20.12.0"
  },
  "scripts": {
    "dev": "vite --clearScreen false",
    "dev:cleanup": "pkill -f ganache",
    "dev:ganache": "ganache --blockTime 1 --chain.chainId=1337 --chain.networkId=5 --chain.hardfork=merge --host '0.0.0.0' -m 'benefit crew supreme gesture quantum web media hazard theory mercy wing kitten' > /dev/null &",
    "dev:setup": "yarn dev:ganache && truffle deploy",
    "dev:goerli": "cross-env NODE_ENV=development REACT_APP_NETWORK=goerli vite",
    "dev:local": "BROWSER=NONE WITH_TEST_STATE=L2 cross-env NODE_ENV=development HTTPS=false REACT_APP_NETWORK=local npm-run-all dev:setup dev dev:cleanup --continue-on-error",
    "dev:mainnet": "cross-env NODE_ENV=development REACT_APP_NETWORK=mainnet vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@tlon/sigil-js": "^1.4.5",
    "@urbit/roller-api": "^0.1.16",
    "azimuth-js": "^0.23.0",
    "bip32": "^4.0.0",
    "bip39": "^3.1.0",
    "bitcoinjs-lib": "^6.1.5",
    "bn.js": "^5.2.1",
    "buffer": "^6.0.3",
    "events": "^3.3.0",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.22.3",
    "sort-by": "^1.2.0",
    "tiny-secp256k1": "^2.2.3",
    "urbit-key-generation": "^0.20.1",
    "vite-plugin-wasm": "^3.3.0",
    "web3": "^4.6.0",
    "web3-utils": "^4.2.2",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@types/events": "^3.0.3",
    "@types/node": "^20.12.5",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.36",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.6"
  }
}
